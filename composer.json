    "yii2",
    "extension",
    "module",
    "chat",
    "telegram",
    "bot"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandr Kozhevnikov",
      "email": "onmotion1@gmail.com",
      "homepage": "http://kozhevnikov.me",
      "role": "Developer"
    },
    {
      "name": "Max Alexandrov",
      "email": "max@7u3.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "yiisoft/yii2": "~2.0.0",
    "longman/telegram-bot": "~0.56.0"
  },
  "autoload": {
    "psr-4": {
      "onmotion\\telegram\\": ""
